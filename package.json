{
  "name": "@monitor/buried-point",
  "version": "1.0.2",
  "description": "前端埋点采集库，支持全埋点、代码埋点、装饰器埋点",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clear": "rm -rf dist",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs && rm -rf public && cp -R docs/.vuepress/dist public",
    "build": "npm run clear && tsc",
    "test": "jest --forceExit --coverage --verbose",
    "test:watch": "npm run test -- --watchAll",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ruixijiejie/buried-point.git"
  },
  "author": "IRIS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ruixijiejie/buried-point/issues"
  },
  "homepage": "https://github.com/ruixijiejie/buried-point",
  "dependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.7",
    "axios": "^0.18.0",
    "moment": "^2.29.1",
    "tslib": "^2.3.1",
    "tslint-config-standard": "^7.1.0",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "build": "^0.1.4",
    "jest": "^23.4.2",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "vuepress": "^0.13.1"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  }
}
